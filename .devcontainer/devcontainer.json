{
	"name": "Container",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "16-bullseye"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"Prisma.prisma",
				"golang.go",
				"PKief.material-icon-theme",
				"bradlc.vscode-tailwindcss",
				"VisualStudioExptTeam.vscodeintellicode",
				"VisualStudioExptTeam.intellicode-api-usage-examples"
			]
		}
	},
	"forwardPorts": [
		3000,
		8080
	],
	"remoteUser": "penguin",
	"features": {
		"docker-in-docker": "latest",
		"git": "latest",
		"golang": "latest"
	}
}
